    "@mdx-js/react": "^1.6.16",
    "@mdx-js/runtime": "^1.6.16",
    "@reach/tabs": "^0.10.3",
    "@sentry/browser": "^5.6.1",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-preval": "^3.0.1",
    "cross-env": "^5.2.0",
    "docsearch.js": "^2.5.2",
    "emoji-regex": "9.0.0",
    "emotion": "^10.0.27",
    "emotion-server": "^10.0.27",
    "front-matter": "^2.3.0",
    "fs-extra": "^6.0.1",
    "github-slugger": "^1.3.0",
    "hotshot": "^1.0.5",
    "lodash": "^4.17.20",
    "next": "^10.2.1",
    "nprogress": "0.2.0",
    "prism-react-renderer": "1.0.2",
    "prop-types": "15.6.2",
    "react": "^16.8.1",
    "react-diff-view": "^2.4.4",
    "react-dom": "^16.8.1",
    "react-player": "^1.11.1",
